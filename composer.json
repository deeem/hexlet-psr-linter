{
  "name": "deeem/hexlet-psr-linter",
  "type": "library",
  "bin": ["bin/psr-linter"],
  "keywords": [
    "lint",
    "linter"
  ],
  "homepage": "https://github.com/deeem/hexlet-psr-linter",
  "license": "MIT",
  "authors": [
    {
      "name": "Dmitry Karpov",
      "email": "karpov.dimon@gmail.com"
    }
  ],
  "require": {
    "nikic/php-parser": ">= 2.1.0",
    "nategood/commando": ">= 0.2.8",
    "symfony/yaml": ">= 3.1.3"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.2",
    "squizlabs/php_codesniffer": "2.*",
    "codeclimate/php-test-reporter": "dev-master"
  },
  "minimum-stability": "stable",
  "autoload": {
    "psr-4": {
      "PsrLinter\\": "src/"
    },
    "files": [
      "src/makeLinter.php",
      "src/lintCli.php",
      "src/report.php"
    ]
  }
}
